{
  "name": "yamdbf",
  "version": "2.6.2",
  "description": "Yet Another Modular Discord Bot Framework: A somewhat simple framework for quickly creating new Discord.js bots.",
  "main": "bin/index",
  "typings": "bin/index.d.ts",
  "scripts": {
    "postinstall": "postinstall-build \"npm run build\"",
    "build": "gulp",
    "docs": "jsdoc -R README.md -c .jsdoc.json -u examples -r bin && jsdoc -c .jsdoc.json -t templates/haruki -r bin -d console > ../yamdbf-docs/docs.json",
    "lint": "tslint './src/**/*.ts'"
  },
  "author": "Zackary Campbell <zajrik@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/zajrik/yamdbf"
  },
  "bugs": {
    "url": "https://github.com/zajrik/yamdbf/issues"
  },
  "license": "MIT",
  "keywords": [
    "discord",
    "framework",
    "bot",
    "client",
    "node",
    "discordapp"
  ],
  "dependencies": {
    "discord.js": "^11.0.0",
    "glob": "7.1.1",
    "node-json-db": "0.7.3",
    "performance-now": "^2.0.0",
    "postinstall-build": "^2.1.3"
  },
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/node": "^6.0.46",
    "@types/node-json-db": "0.0.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^3.1.5",
    "jsdoc": "github:zajrik/jsdoc",
    "jsdoc-strip-async-await": "^0.1.0",
    "minami": "github:zajrik/minami",
    "tslint": "^3.15.1",
    "typescript": "^2.2.1"
  }
}
